  "private": true,
  "packageManager": "pnpm@9.15.9",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "postinstall": "node scripts/prisma-run.mjs generate",
    "dev": "next dev",
    "build": "next build",
    "vercel-build": "node scripts/prisma-run.mjs db push --accept-data-loss || true && next build",
    "start": "next start",
    "lint": "eslint",
    "format": "prettier . --write",
    "format:check": "prettier . --check",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "db:generate": "node scripts/prisma-run.mjs generate",
    "db:push": "node scripts/prisma-run.mjs db push",
    "db:migrate:dev": "node scripts/prisma-run.mjs migrate dev",
    "db:migrate:deploy": "node scripts/prisma-run.mjs migrate deploy",
    "db:studio": "node scripts/prisma-run.mjs studio",
    "import:data": "tsx scripts/import-xlsx.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.19.1",
    "@radix-ui/react-accordion": "^1.2.12",
